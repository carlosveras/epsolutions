@model IEnumerable<EPSolutions.Models.ItensRomaneio>
<h1 class="mt-1" style="font-size: 20px;">Notícias</h1>

<div class="row">
    <div class="col-12 col-md-6">
        <form method="get" class="form-inline justify-content-center justify-content-md-start mb-sm-3 mb-md-0" action="/Romaneio/Entregar" method="get" id="formBusca">
            <input type="text" id="searchInput" placeholder="Pesquisar por Romaneio" class="form-control form-control-sm mr-sm-2" name="id" />
            <button class="btn btn-sm btn-success my-2 my-sm-0 form-control form-control-sm" id="searchButton">Buscar</button>
            @await Html.PartialAsync("_ItensRomaneioPartial")
        </form>
    </div>
</div>


